<?xml version="1.0" encoding="UTF-8"?>
<styles xmlns="http://mycila.com/license/styles/1.0">

    <style name="javadoc">
        <description>Javadoc header style</description>
        <definition>
            <begining>/**</begining>
            <startLine> * </startLine>
            <ending> */</ending>
        </definition>
        <detection>
            <skip>ss</skip>
            <begining>(\s|\t)*/\*.*$</begining>
            <ending>.*\*/(\s|\t)*$</ending>
        </detection>
    </style>

</styles>
